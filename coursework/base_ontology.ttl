@prefix : <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown> .

<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown> rdf:type owl:Ontology ;
                                                            rdfs:comment "Ontology designed for modelling of 'Pizza_data' for Semnatic Web Technologies and Knowledge Graphs coursework, MSc in Data Science, City, University of London"@en ,
                                                                         """Reference for resource which aided with creating ontology and navigating Protege:

https://www.youtube.com/watch?v=ZgTR2B1gkdM"""@en ;
                                                            rdfs:seeAlso "Report, and Supporting notebook"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Created_by
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Created_by> rdf:type owl:AnnotationProperty ;
                                                                      rdfs:range rdfs:Literal .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#Literal
rdfs:Literal owl:equivalentClass xsd:float .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_base
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_base> rdf:type owl:ObjectProperty ;
                                                                    rdfs:subPropertyOf <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_ingredient> ;
                                                                    owl:inverseOf <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Is_base_of> ;
                                                                    rdf:type owl:FunctionalProperty ;
                                                                    rdfs:range <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Pizza_base> ;
                                                                    rdfs:comment "Specifies the type of base the pizza has."@en .


###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_ingredient
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_ingredient> rdf:type owl:ObjectProperty ;
                                                                          owl:inverseOf <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Is_ingredient_of> ;
                                                                          rdfs:comment "Contains or is made up of a certain ingredient"@en .


###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_location
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_location> rdf:type owl:ObjectProperty ;
                                                                        owl:inverseOf <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Is_located_in> ;
                                                                        rdf:type owl:TransitiveProperty ;
                                                                        rdfs:comment "Specifies the location(s) within the location at hand."@en .


###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_topping
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_topping> rdf:type owl:ObjectProperty ;
                                                                       rdfs:subPropertyOf <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_ingredient> ;
                                                                       owl:inverseOf <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Is_topping_of> ;
                                                                       rdfs:comment "Specifies the topping(s) the pizzas has."@en .


###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Is_base_of
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Is_base_of> rdf:type owl:ObjectProperty ;
                                                                      rdfs:subPropertyOf <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Is_ingredient_of> ;
                                                                      rdf:type owl:InverseFunctionalProperty ;
                                                                      rdfs:domain <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Pizza_base> ;
                                                                      rdfs:range <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Pizza> ;
                                                                      owl:propertyDisjointWith <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Is_topping_of> ;
                                                                      rdfs:comment "Specifies the pizza(s) this base is a component of."@en .


###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Is_ingredient_of
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Is_ingredient_of> rdf:type owl:ObjectProperty ;
                                                                            rdfs:comment "Specifies the pizza(s) that this ingredient is a component of."@en .


###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Is_located_in
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Is_located_in> rdf:type owl:ObjectProperty ,
                                                                                  owl:TransitiveProperty ;
                                                                         rdfs:comment "Sepcifies the location(s) that this location is located within."@en .


###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Is_topping_of
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Is_topping_of> rdf:type owl:ObjectProperty ;
                                                                         rdfs:subPropertyOf <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Is_ingredient_of> ;
                                                                         rdfs:domain <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Pizza_topping> ;
                                                                         rdfs:range <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Pizza> ;
                                                                         rdfs:comment "Specifies the pizza(s) this ingredient is the topping of."@en .


###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Served_at
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Served_at> rdf:type owl:ObjectProperty ;
                                                                     owl:inverseOf <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Serves> ;
                                                                     rdf:type owl:FunctionalProperty ;
                                                                     rdfs:domain <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Pizza> ;
                                                                     rdfs:range <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Establishment> ;
                                                                     rdfs:comment "Specifies the estblishment that this particular pizza is served at."@en .


###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Serves
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Serves> rdf:type owl:ObjectProperty ,
                                                                           owl:InverseFunctionalProperty ;
                                                                  rdfs:domain <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Establishment> ;
                                                                  rdfs:range <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Pizza> ;
                                                                  rdfs:comment "Specfies the pizzas that this establishment serves."@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_address
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_address> rdf:type owl:DatatypeProperty ;
                                                                       rdfs:subPropertyOf owl:topDataProperty ;
                                                                       rdf:type owl:FunctionalProperty ;
                                                                       rdfs:domain <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Establishment> ;
                                                                       rdfs:range rdfs:Literal ;
                                                                       rdfs:comment "The address of the establishment."@en .


###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_name
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_name> rdf:type owl:DatatypeProperty ,
                                                                             owl:FunctionalProperty ;
                                                                    rdfs:range rdfs:Literal ;
                                                                    rdfs:comment "The name of the pizza, as specified in the data set"@en .


###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_postcode
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_postcode> rdf:type owl:DatatypeProperty ;
                                                                        rdfs:subPropertyOf owl:topDataProperty ;
                                                                        rdf:type owl:FunctionalProperty ;
                                                                        rdfs:domain <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Establishment> ;
                                                                        rdfs:range rdfs:Literal ;
                                                                        rdfs:comment "The postcode of the establishment."@en .


###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_price
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_price> rdf:type owl:DatatypeProperty ,
                                                                              owl:FunctionalProperty ;
                                                                     rdfs:domain <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Pizza> ;
                                                                     rdfs:range xsd:float ;
                                                                     rdfs:comment "The price of the pizza, as specified on the menu (possibly N/A), as type xsd:float."@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Americana
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Americana> rdf:type owl:Class ;
                                                                     rdfs:subClassOf <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Meat_pizza> ,
                                                                                     <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Specific_pizza> ,
                                                                                     [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_topping> ;
                                                                                       owl:someValuesFrom <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Cheese_topping>
                                                                                     ] ,
                                                                                     [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Has_topping> ;
                                                                                       owl:someValuesFrom <http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Pepperoni>
                                                                                     ] ;
                                                                     rdfs:isDefinedBy "Tomato base, cheese topping, pepperoni topping. Can contain other ingredients/has different interpreations."@en ;
                                                                     rdfs:label "Americana"@en .


###  http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Anchovy
<http://www.semanticweb.org/jake/ontologies/2021/2/jbrown/Anchovy> rdf:type owl:Class ;
